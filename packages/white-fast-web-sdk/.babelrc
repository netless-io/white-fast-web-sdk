{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-object-rest-spread",
        "@babel/transform-runtime",
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": true
        }, "ant"]
    ]
}
